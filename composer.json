        "ibexa/image-editor": "~4.0.0@rc",
        "ibexa/oauth2-client": "~4.0.0@rc",
        "ibexa/installer": "~4.0.0@rc",
        "ibexa/product-catalog": "~4.0.0@rc",
        "ibexa/scheduler": "~4.0.0@rc",
        "ibexa/calendar": "~4.0.0@rc",
        "ibexa/connector-dam": "~4.0.0@rc",
        "ibexa/elasticsearch": "~4.0.0@rc",
        "ibexa/fastly": "~4.0.0@rc",
        "ibexa/icons": "~4.0.0@rc",
        "ibexa/personalization": "~4.0.0@rc",
        "ibexa/version-comparison": "~4.0.0@rc",
        "ibexa/workflow": "~4.0.0@rc",
        "ibexa/taxonomy": "~4.0.0@rc",
